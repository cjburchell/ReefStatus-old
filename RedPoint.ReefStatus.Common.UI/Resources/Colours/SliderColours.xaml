<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="SliderThumbBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFE3F1E0" Offset="0"/>
        <GradientStop Color="#FF1A9654" Offset="0.5"/>
        <GradientStop Color="#FF126437" Offset="0.5"/>
        <GradientStop Color="#FF1A9654" Offset="1"/>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="SliderThumbBorder" Color="#FF056938"/>
    
    <SolidColorBrush x:Key="SliderTrackNormalBackground" Color="#FFCCCCCC"/>

    <SolidColorBrush x:Key="SliderTrackNormalBorder" Color="#FF666666"/>
    
    <SolidColorBrush x:Key="SliderTickColor" Color="#FFCCCCCC"/>

</ResourceDictionary>