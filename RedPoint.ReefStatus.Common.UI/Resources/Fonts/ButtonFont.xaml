<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="DefaultFontFamily">Segoe UI</FontFamily>

    <!-- 9pt -->
    <System:Double x:Key="DefaultFontSize">12</System:Double>

    <!-- 11pt -->
    <System:Double x:Key="TitlebarFontSize">14.667</System:Double>

    <!-- 11pt -->
    <System:Double x:Key="DefaultFontSize2">14.667</System:Double>

    <!-- 20pt -->
    <System:Double x:Key="DefaultFontSize3">26.67</System:Double>

    <!-- 42pt -->
    <System:Double x:Key="DefaultFontSize4">56</System:Double>

</ResourceDictionary>